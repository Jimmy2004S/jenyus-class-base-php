{
  "name": "jenyus/class-base-php",
  "description": "El paquete provee una clase base para manejar algunas consultas de modelos de forma dinamica. Ademas de ya traer una estructura base para proyectos mvc",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "vlucas/phpdotenv": "5.6",
    "graham-campbell/result-type": "^1.1",
    "phpoption/phpoption": "^1.9",
    "symfony/polyfill-mbstring": "^1.31",
    "symfony/deprecation-contracts": "^3.5",
    "symfony/http-foundation": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Jenyus\\Base\\": "src/Base/",
      "Jenyus\\Exception\\": "src/Exception/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jimmy Jimenez"
    }
  ],
  "minimum-stability": "stable"
}
